<?xml version="1.0" encoding="UTF-8"?>

<!--文件作用：
    1. 控制组件是否参与编译
    2. 控制组件aar或源码的切换
    3. 编译时的一些开关
    联动子组件无需在此列出，自动使用父组件的配置。
-->
<configs>
    <project>
        <!--aar:使用aar，include:组件参与编译，project:组件名-->
        <component aar="true" include="true" project="module_main" />
        <component aar="true" include="true" project="module_login" />
        <component aar="true" include="true" project="module_luckydraw" />

        <component aar="false" include="true" project="module_demo" />

        <component aar="true" include="true" project="lib_common" />
    </project>

    <buildOptions>
        <!--编译混淆开关-->
        <enable_proguard>false</enable_proguard>
        <!--全部使用源码-->
        <all_sourcecode>true</all_sourcecode>
    </buildOptions>

</configs>
